{
  "search.exclude": {
    "**/dist": true,
    "**/lib": true,
    "docs/docs/api/**": true,
    "docs/build/**": true,
    "docs/.docusaurus/**": true
  },
  "files.exclude": {
    "**/.turbo": true,
    "**/lib": true,
    "**/node_modules": true,
    "docs/docs/api/**": true,
    "docs/build/**": true,
    "docs/.docusaurus/**": true
  },
  "cSpell.words": ["cardsgame", "colyseus"],
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.preferences.autoImportFileExcludePatterns": [
    "index.ts",
    "lib/**"
  ],
  "jest.jestCommandLine": "node_modules/.bin/jest --config config/jest.config.cjs --silent "
}

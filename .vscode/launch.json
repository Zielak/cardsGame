  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run example server",
      "runtimeExecutable": "node",
      "program": "${workspaceFolder}/packages/examples/dist/server/index.js",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "-- old -- Debug example project",
      "args": ["${workspaceFolder}/packages/examples/server/index.ts"],
      "runtimeArgs": ["-r", "${workspaceFolder}/scripts/ts-node.js"],
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}

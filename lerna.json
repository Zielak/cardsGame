{
  "version": "0.12.7",
  "packages": ["packages/*"],
  "command": {
    "publish": {
      "conventionalCommits": true,
      "ignoreChanges": ["*.md", "test/**"],
      "message": "chore(release): publish %s"
    },
    "version": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "createRelease": "github"
    }
  }
}

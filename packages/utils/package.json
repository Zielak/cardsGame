{
  "name": "@cardsgame/utils",
  "version": "1.7.1",
  "description": "",
  "author": "Darek Greenly (https://darekgreenly.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zielak/cardsGame.git"
  },
  "bugs": {
    "url": "https://github.com/zielak/cardsGame/issues"
  },
  "main": "lib/cjs/index.cjs",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "lib",
    "readme.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json & tsc -p tsconfig.build.cjs.json & wait && ../../scripts/mapToCjs.sh ./lib/cjs",
    "build:watch": "echo \"only for ESM!\" && tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@cardsgame/types": "^1.7.1",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@cardsgame/base-configs": "^1.7.1",
    "@types/node": "^18.11.0"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.cjs",
      "types": "./lib/types/index.d.ts"
    },
    "./array": {
      "import": "./lib/esm/array.js",
      "require": "./lib/cjs/array.cjs",
      "types": "./lib/types/array.d.ts"
    },
    "./entities": {
      "import": "./lib/esm/entities.js",
      "require": "./lib/cjs/entities.cjs",
      "types": "./lib/types/entities.d.ts"
    },
    "./functions": {
      "import": "./lib/esm/functions.js",
      "require": "./lib/cjs/functions.cjs",
      "types": "./lib/types/functions.d.ts"
    },
    "./logs": {
      "import": "./lib/esm/logs.js",
      "require": "./lib/cjs/logs.cjs",
      "types": "./lib/types/logs.d.ts"
    },
    "./numbers": {
      "import": "./lib/esm/numbers.js",
      "require": "./lib/cjs/numbers.cjs",
      "types": "./lib/types/numbers.d.ts"
    },
    "./objects": {
      "import": "./lib/esm/objects.js",
      "require": "./lib/cjs/objects.cjs",
      "types": "./lib/types/objects.d.ts"
    },
    "./random": {
      "import": "./lib/esm/random.js",
      "require": "./lib/cjs/random.cjs",
      "types": "./lib/types/random.d.ts"
    },
    "./strings": {
      "import": "./lib/esm/strings.js",
      "require": "./lib/cjs/strings.cjs",
      "types": "./lib/types/strings.d.ts"
    },
    "./utils": {
      "import": "./lib/esm/utils.js",
      "require": "./lib/cjs/utils.cjs",
      "types": "./lib/types/utils.d.ts"
    }
  }
}

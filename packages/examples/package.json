{
  "name": "@cardsgame/examples",
  "version": "0.1.4",
  "description": "",
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "dist",
    "public",
    "server"
  ],
  "scripts": {
    "start": "echo 'http://localhost:1234' && npm run client | npm run server",
    "client": "parcel client/index.html --out-dir dist/client",
    "server": "npm run build:server && nodemon dist/server/index.js --watch server",
    "server:ts": "nodemon -x ts-node --project server/tsconfig.json --files server/index.ts --watch server",
    "build": "npm run build:client && npm run build:server",
    "build:client": "parcel build client/index.html --out-dir dist/client",
    "build:server": "tsc -b server"
  },
  "dependencies": {
    "@cardsgame/client": "^0.1.4",
    "@cardsgame/server": "^0.1.4",
    "@cardsgame/types": "^0.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "chalk": "^2.4.2",
    "nodemon": "^1.18.10",
    "parcel-bundler": "^1.12.2",
    "parcel-plugin-clean-dist": "0.0.6",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}

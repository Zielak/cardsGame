{
  "name": "cards-game-docs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "arch": "./scripts/structurizr.sh",
    "typecheck": "tsc",
    "typedoc": "concurrently -k 'npm:typedoc:*'",
    "typedoc:server": "typedoc --options ./typedoc/server.typedoc.js --watch",
    "typedoc:client": "typedoc --options ./typedoc/client.typedoc.js --watch",
    "typedoc:utils": "typedoc --options ./typedoc/utils.typedoc.js --watch",
    "typedoc:serverTesting": "typedoc --options ./typedoc/serverTesting.typedoc.js --watch"
  },
  "dependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-plugin-typedoc": "^0.17.5",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "structurizr-typescript": "^1.0.11",
    "typedoc": "^0.23.8"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "^1.0.6",
    "concurrently": "^7.3.0",
    "jsdoc-to-markdown": "^7.1.1",
    "typedoc-plugin-markdown": "^3.13.4",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
